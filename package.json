  "scripts": {
    "build": "rm -rf dist && webpack --bail --progress --profile",
    "clean-cache": "npm cache clean",
    "clean": "rm -rf node_modules dist ./test/results ./test/coverage",
    "clean-install": "npm run clean && npm install",
    "eslint": "eslint app test/e2e-protractor examples webpack",
    "jsb": "node ./utils/js-beautify --beautify",
    "jsb-verify": "node ./utils/js-beautify --verify",
    "json-verify": "node ./utils/json-verify.js",
    "kill": "lsof -i tcp:8000 | grep LISTEN | awk '{print $2}' | xargs kill",
    "languages-verify": "node ./utils/languages-verify.js",
    "preinstall": "node ./utils/semver-check.js",
    "postinstall": "npm run ts-spec-delete && npm run typings",
    "server": "lite-server -c config/lite-server-dev.js",
    "server-dist": "lite-server -c config/lite-server-dist.js",
    "test": "karma start",
    "test-watch": "karma start --auto-watch --no-single-run",
    "test-debug": "karma start --no-single-run --debug",
    "ts-spec-delete": "node ./utils/ts-spec-delete.js",
    "typings": "rm -rf typings && typings install",
    "webdriver": "webdriver-manager update"
  },
