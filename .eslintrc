{
  "extends": "airbnb/legacy",
  "env": {
    "jasmine": true
  },
  "globals": {
    "_": true,
    "$": true,
    "AmCharts": true,
    "angular": true,
    "bard": true,
    "getJSONFixture": true,
    "inject": true,
    "jstz": true,
    "moment": true,
    "phoneUtils": true,
    "sinon": true
  },
  "rules": {
    "block-scoped-var": 0,
    "brace-style": 0,
    "camelcase": 0,
    "comma-dangle": 0,
    "consistent-return": 0,
    "curly": 0,
    "default-case": 0,
    "dot-notation": 0,
    "eqeqeq": 0,
    "func-names": 0,
    "guard-for-in": 0,
    "indent": 0,
    "new-cap": 0,
    "no-alert": 0,
    "no-cond-assign": 0,
    "no-console": 0,
    "no-else-return": 0,
    "no-loop-func": 0,
    "no-nested-ternary": 0,
    "no-param-reassign": 0,
    "no-shadow": 0,
    "no-spaced-func": 0,
    "no-throw-literal": 0,
    "no-trailing-spaces": 0,
    "no-unused-vars": 0,
    "no-use-before-define": 0,
    "one-var": 0,
    "padded-blocks": 0,
    "quotes": 0,
    "radix": 0,
    "spaced-comment": 0,
    "strict": 0,
    "vars-on-top": 0,
    "yoda": 0
  }
}
